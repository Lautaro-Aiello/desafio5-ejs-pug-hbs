<h2>Productos ingresados</h2>

<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr> <th>TITLE</th> <th>PRICE</th> <th>URL</th> </tr>
            <% for (const product of productos) {%>
            <tr>
                <td><%=product.title%></td>
                <td> $ <%=product.price%></td>
                <td><%=product.thumbnail%></td>
            </tr>
            <%};%>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">No hay productos cargados</h1>
<% } %>

<a href="/"> <button class="btn btn-danger"> Ingresar producto </button> </a>